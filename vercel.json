{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist/don-music"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/convert",
            "dest": "/api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}